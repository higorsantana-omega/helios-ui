{
  "name": "@helios-ui/react",
  "version": "1.0.3",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.mjs",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js",
      "default": "./dist/src/index.mjs"
    },
    "./styles.css": "./dist/src/styles/index.css",
    "./icons": {
      "types": "./dist/src/icons/index.d.ts",
      "import": "./dist/src/icons/index.mjs",
      "require": "./dist/src/icons/index.js",
      "default": "./dist/src/icons/index.mjs"
    },
    "./utils": {
      "types": "./dist/src/utils.d.ts",
      "import": "./dist/src/utils.mjs",
      "require": "./dist/src/utils.js",
      "default": "./dist/src/utils.mjs"
    },
    "./tailwind-preset": {
      "types": "./dist/tailwind.config.d.ts",
      "import": "./dist/tailwind.config.mjs",
      "require": "./dist/tailwind.config.js",
      "default": "./dist/tailwind.config.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@helios-ui/tokens": "*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.4.0"
  }
}
